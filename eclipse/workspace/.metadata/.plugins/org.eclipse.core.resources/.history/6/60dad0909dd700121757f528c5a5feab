package analyse;

public class WordInfo {

	private int corpusFrequency;
	private String word;
	private String type;
	private String classe;
	private String matiere;
	
	public WordInfo(String word){
		this.corpusFrequency=1;
		this.word=word;
		this.type=type;
		this.classe=classe;
		this.matiere=matiere;
	}
}
