package analyse;

import java.util.HashMap;
import java.util.Map;

public class Words {
	private int corpusFrequency;
	private String word;
	private String type;
	private Map<String,DocInfo> docPresence = new HashMap<String,DocInfo>();
	
	public Words(String word,String type){
		this.corpusFrequency=1;
		this.word=word;
		this.type=type;

	}
}
